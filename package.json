{
  "name": "kadabra-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:vite\"",
    "dev:vite": "vite",
    "dev:server": "tsx server.ts",
    "dev:vercel": "npx vercel dev . --yes",
    "dev:mock": "VITE_USE_MOCK_DATA=true npm run dev",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:themes": "reshaped theming --output src/themes",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/server/db/seed.ts"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.5",
    "@tanstack/react-query": "^5.90.6",
    "@dnd-kit/core": "^6.2.1",
    "@dnd-kit/sortable": "^9.0.0",
    "@dnd-kit/utilities": "^3.2.1",
    "@trpc/client": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@types/ws": "^8.18.1",
    "@vercel/postgres": "^0.10.0",
    "cookie": "^1.0.2",
    "drizzle-orm": "^0.44.7",
    "express": "^5.1.0",
    "google-auth-library": "^10.5.0",
    "jose": "^6.1.0",
    "nanoid": "^5.1.6",
    "openai": "^6.8.0",
    "papaparse": "^5.5.3",
    "postgres": "^3.4.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.5",
    "reshaped": "^3.8.7",
    "ws": "^8.18.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/express": "^5.0.5",
    "@types/node": "^24.6.0",
    "@types/papaparse": "^5.5.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.4.49",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vercel": "^48.8.2",
    "vite": "^7.1.7"
  }
